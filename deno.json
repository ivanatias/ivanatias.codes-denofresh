{
  "tasks": {
    "start": "deno run -A --watch=tailwind.css,routes/ dev.ts"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "fmt": {
    "files": {
      "include": [
        "."
      ]
    },
    "options": {
      "useTabs": false,
      "lineWidth": 80,
      "indentWidth": 2,
      "singleQuote": true,
      "proseWrap": "preserve",
      "semiColons": false
    }
  },
  "imports": {
    "$fresh/": "https://deno.land/x/fresh@1.2.0/",
    "$std/": "https://deno.land/std@0.180.0/",
    "preact": "https://esm.sh/preact@10.15.1",
    "preact/": "https://esm.sh/preact@10.15.1/",
    "preact-render-to-string": "https://esm.sh/*preact-render-to-string@5.2.4",
    "@preact/signals": "https://esm.sh/*@preact/signals@1.1.3",
    "@preact/signals-core": "https://esm.sh/*@preact/signals-core@1.2.3",
    "sanity/client": "https://esm.sh/@sanity/client@4.0.1",
    "sanity/image-url": "https://esm.sh/@sanity/image-url@1.0.2",
    "sanity/asset-utils": "https://esm.sh/@sanity/asset-utils@1.3.0",
    "portabletext": "https://esm.sh/react-portable-text@0.6.0",
    "refractor/": "https://esm.sh/refractor@4.8.0/",
    "refractor": "https://esm.sh/refractor@4.8.0",
    "hast-to-util-html": "https://esm.sh/hast-util-to-html@8.0.4",
    "components/": "./components/",
    "utils/": "./utils/",
    "lib/": "./lib/",
    "services/": "./services/",
    "models/": "./models/",
    "constants/": "./constants/",
    "contexts/": "./contexts/",
    "islands/": "./islands/"
  }
}
